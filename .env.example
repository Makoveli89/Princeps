# Princeps Brain Layer - Environment Configuration

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/princeps_brain

# Embedding Model
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json  # or "text"
LOG_FILE=./logs/princeps.log

# Security
SECRET_KEY=your-secret-key-change-in-production
API_KEY_PREFIX=pk

# Multi-tenancy
DEFAULT_TENANT=default

# Performance
BATCH_SIZE=100
MAX_WORKERS=4

# Optional: ML Model Settings
USE_TRANSFORMERS=true
USE_SPACY=true
SPACY_MODEL=en_core_web_sm
